<h1>Add a picture</h1>
<%= form_for @picture do |f| %>
  <p>
    <%= label_tag "artist" %><br>
    <%= f.text_field :artist %>
  </p>
  <p>
    <%= label_tag "title" %><br>
    <%= f.text_field :title %>
  </p>
  <p>
    <%= label_tag "url" %><br>
    <%= f.text_field :url %>
  </p>
  <p>
    <%= f.submit "Save" %>
  </p>
<% end %>
